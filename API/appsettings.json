{
  "GamesDatabaseSettings": {
    "GamesCollectionName": "Game",
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "GamesDB"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "CORS": {
    "AllowedHosts": [
      "http://preprod.com",
      "http://test.com",
      "http://prod.com",
      "http://localhost:5000",
      "http://localhost:5001"

    ]
  }
}
